{
    "name": "daicar/laravel-event-sender",
    "description": "Queue message sender",
    "type": "library",
    "require": {
        "php": "^7.2.5 | ^8.0",
        "aws/aws-sdk-php": "^3.0",
        "illuminate/support": "^5.8|^6|^7|^8|^9|^10|^11|^12"
    },
    "require-dev": {
        "laravel/framework": "5.0 - 5.8 | ^6.0 | ^7.0 | ^8.0 | ^9.0",
        "phpstan/phpstan": "^1.12",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Daicar\\EventSender\\Laravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Daicar\\EventSender\\Laravel\\EventSenderServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
